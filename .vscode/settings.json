{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": true,
  "files.encoding": "utf8",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "search.exclude": {
    "dist/**": true,
    "node_modules/**": true,
    "typings/**": true
  },
  "files.exclude": {
    "**/*.js": { "when": "$(basename).ts"}
  }
}
